{
	"app": {
		"pidfile": "/tmp/pndcc.pid",
		"rootapp": "pkexec",
		"systemcontrol": "systemctl"
	},
	"services": {
		"nginx": {
			"1": {
				"name": "nginx",
				"pid": "/run/nginx.pid"
			},
			"2": {
				"name": "php-fpm",
				"pid": "/opt/pndcc/pids/php-fpm.pid"
			}
		},
		"httpd": {
			"1": {
				"name": "httpd",
				"pid": "/run/httpd/httpd.pid"
			}
		},
		"postgres": {
			"1": {
				"name": "postgresql",
				"pid": "/opt/pndcc/pids/postgres.pid"
			}
		},
		"mysqld": {
			"1": {
				"name": "mysqld",
				"pid": "/run/mysqld/mysqld.pid"
			}
		}
	}
}
